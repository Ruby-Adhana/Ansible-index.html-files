sudo kubeadm reset
sudo rm -rf /etc/kubernetes
sudo rm -rf /var/lib/etcd
sudo rm -f /etc/cni/net.d/*
sudo rm -rf ~/.kube
sudo systemctl stop kubelet
sudo systemctl disable kubelet
sudo iptables -F
sudo iptables -t nat -F
sudo iptables -t mangle -F
sudo iptables -X
sudo ipvsadm --clear
apt update -y
